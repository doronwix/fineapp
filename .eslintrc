{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        },
        "plugins": [
            "react"
        ]
    },
    

    "env": {
        "es6": true
    }
    
}