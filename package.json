{
  "name": "dcfserver",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.4"
  },
  "description": "extract dcf values from known API's",
  "main": "index.js",
  "scripts": {
    "dev": "set DEBUG=app & nodemon server.js",
    "start": "node server.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "everpolate": "^0.0.3",
    "express": "^4.17.1",
    "html-extract-js": "^0.1.9",
    "ml-regression-polynomial": "^2.1.0",
    "mongoose": "^5.9.29",
    "node-worker-threads-pool": "^1.4.1",
    "parse-xbrl-10k": "git+https://github.com/doronwix/parse-xbrl-10k.git",
    "yahoo-stock-prices": "^1.0.1"
  },
  "devDependencies": {
    "jest": "^25.5.4",
    "standard": "^12.0.1",
    "supertest": "^4.0.2"
  },
  "nodemonConfig": {
    "restartble": "rs",
    "ignore": "node_modules/**/node_modules",
    "delay": "1500",
    "env": {
      "NODE_ENV": "development",
      "PORT": "4000"
    }
  }
}
